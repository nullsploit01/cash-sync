{
  "expo": {
    "name": "CashSync",
    "slug": "cashsync",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "scheme", // TBD
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "inkeep.nullsploit.cashsync" // update identifier after bug fix
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "inkeep.nullsploit.cashsync" // update identifier after bug fix
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "51b02019-46e5-484a-9114-2634ee47d8ef"
      }
    },
    "owner": "nullsploit01",
    "plugins": [
      "expo-font",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ],
      "expo-router"
    ]
  }
}
